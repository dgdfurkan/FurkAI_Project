{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/logs.js": {
      "maxDuration": 10
    },
    "api/subscribe.js": {
      "maxDuration": 10
    },
    "api/send.js": {
      "maxDuration": 10
    },
    "api/schedule.js": {
      "maxDuration": 10
    }
  }
}
