{
  "version": 2,
  "functions": {
    "api/subscribe.js": {
      "runtime": "nodejs18.x"
    },
    "api/send.js": {
      "runtime": "nodejs18.x"
    },
    "api/schedule.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
